####################################################################################################
# PROJECT: Samples.Integration
####################################################################################################

ifeq ($(V), 1)
 Q =
else
 Q = @
endif

$(PREFIX)/Samples/Integration/Projects/linux_x86/_build/Profile/pch.h.gch: $(PREFIX)/Samples/Integration/Projects/linux_x86/pch.h
	@echo [PH] Samples.Integration/$(notdir $<)
	$(Q)g++ $(CPPFLAGS_PROFILE) -I$(PREFIX)/Gui/Core/Include/ -I$(PREFIX)/App/Display/Include/ -I$(PREFIX)/Gui/DependencySystem/Include/ -I$(PREFIX)/Drawing/Types/Include/ -I$(PREFIX)/Core/Kernel/Include/ -I$(PREFIX)/Gui/Providers/Include/ -I$(PREFIX)/Math/VectorMath/Include/ -I$(PREFIX)/Gui/CachedFontProvider/Include/ -I$(PREFIX)/../../Include/ -I$(PREFIX)/Render/RenderContext/Include/ -I$(PREFIX)/Core/NSTL/Include/ -I$(PREFIX)/Core/Nullable/Include/ -I$(PREFIX)/Samples/Integration/Include/ -I$(PREFIX)/Math/Utils/Include/ -I$(PREFIX)/App/Launcher/Include/ -I$(PREFIX)/../../3rdParty/EASTL/local/include/ -I$(PREFIX)/Samples/Integration/Projects/linux_x86/_build/Profile -DNS_BUILD -DNS_CONFIG=Profile -DNS_VERSION=2.1.0f1 -DNS_APP_FRAMEWORK -DNS_PROFILE_CFG -DNS_ASSERTS_ENABLED=0 -DNS_INSTRUMENTED_ENABLED=1 -DNS_MINIMUM_LOG_LEVEL=2 -march=i686 -m32 -c -MMD -fPIC -o $@ $<

$(PREFIX)/Samples/Integration/Projects/linux_x86/_build/Profile/%.o: $(PREFIX)/Samples/Integration/Src/%.cpp $(PREFIX)/Samples/Integration/Projects/linux_x86/_build/Profile/pch.h.gch
	@echo [CC] Samples/Integration/$(notdir $<)
	$(Q)g++ $(CPPFLAGS_PROFILE) -I$(PREFIX)/Gui/Core/Include/ -I$(PREFIX)/App/Display/Include/ -I$(PREFIX)/Gui/DependencySystem/Include/ -I$(PREFIX)/Drawing/Types/Include/ -I$(PREFIX)/Core/Kernel/Include/ -I$(PREFIX)/Gui/Providers/Include/ -I$(PREFIX)/Math/VectorMath/Include/ -I$(PREFIX)/Gui/CachedFontProvider/Include/ -I$(PREFIX)/../../Include/ -I$(PREFIX)/Render/RenderContext/Include/ -I$(PREFIX)/Core/NSTL/Include/ -I$(PREFIX)/Core/Nullable/Include/ -I$(PREFIX)/Samples/Integration/Include/ -I$(PREFIX)/Math/Utils/Include/ -I$(PREFIX)/App/Launcher/Include/ -I$(PREFIX)/../../3rdParty/EASTL/local/include/ -I$(PREFIX)/Samples/Integration/Projects/linux_x86/_build/Profile -DNS_BUILD -DNS_CONFIG=Profile -DNS_VERSION=2.1.0f1 -DNS_APP_FRAMEWORK -DNS_PROFILE_CFG -DNS_ASSERTS_ENABLED=0 -DNS_INSTRUMENTED_ENABLED=1 -DNS_MINIMUM_LOG_LEVEL=2 -march=i686 -m32 -c -MMD -fPIC -o $@ $< -include pch.h

$(PREFIX)/Samples/Integration/Projects/linux_x86/_build/Profile/%.o: $(PREFIX)/Samples/Integration/Src/%.c
	@echo [CC] Samples/Integration/$(notdir $<)
	$(Q)gcc $(CFLAGS_PROFILE) -I$(PREFIX)/Gui/Core/Include/ -I$(PREFIX)/App/Display/Include/ -I$(PREFIX)/Gui/DependencySystem/Include/ -I$(PREFIX)/Drawing/Types/Include/ -I$(PREFIX)/Core/Kernel/Include/ -I$(PREFIX)/Gui/Providers/Include/ -I$(PREFIX)/Math/VectorMath/Include/ -I$(PREFIX)/Gui/CachedFontProvider/Include/ -I$(PREFIX)/../../Include/ -I$(PREFIX)/Render/RenderContext/Include/ -I$(PREFIX)/Core/NSTL/Include/ -I$(PREFIX)/Core/Nullable/Include/ -I$(PREFIX)/Samples/Integration/Include/ -I$(PREFIX)/Math/Utils/Include/ -I$(PREFIX)/App/Launcher/Include/ -I$(PREFIX)/../../3rdParty/EASTL/local/include/ -I$(PREFIX)/Samples/Integration/Projects/linux_x86/_build/Profile -DNS_BUILD -DNS_CONFIG=Profile -DNS_VERSION=2.1.0f1 -DNS_APP_FRAMEWORK -DNS_PROFILE_CFG -DNS_ASSERTS_ENABLED=0 -DNS_INSTRUMENTED_ENABLED=1 -DNS_MINIMUM_LOG_LEVEL=2 -march=i686 -m32 -c -MMD -fPIC -o $@ $<

$(PREFIX)/Samples/Integration/Projects/linux_x86/_build/Profile/Settings.xaml.bin.h: $(PREFIX)/Samples/Integration/Src/Settings.xaml
	@echo [BH] Samples/Integration/$(notdir $<)
	$(Q)mkdir -p $(PREFIX)/Samples/Integration/Projects/linux_x86/_build/Profile 
	$(Q)$(PREFIX)/../Tools/Bin2h/bin2h.py $(PREFIX)/Samples/Integration/Src/Settings.xaml > $(PREFIX)/Samples/Integration/Projects/linux_x86/_build/Profile/Settings.xaml.bin.h 

$(PREFIX)/Samples/Integration/Projects/linux_x86/_build/Profile/HermeneusOne-Regular.ttf.bin.h: $(PREFIX)/Samples/Integration/Src/HermeneusOne-Regular.ttf
	@echo [BH] Samples/Integration/$(notdir $<)
	$(Q)mkdir -p $(PREFIX)/Samples/Integration/Projects/linux_x86/_build/Profile 
	$(Q)$(PREFIX)/../Tools/Bin2h/bin2h.py $(PREFIX)/Samples/Integration/Src/HermeneusOne-Regular.ttf > $(PREFIX)/Samples/Integration/Projects/linux_x86/_build/Profile/HermeneusOne-Regular.ttf.bin.h 

$(BINDIR)/Samples.Integration: $(PREFIX)/Samples/Integration/Projects/linux_x86/_build/Profile/Settings.xaml.bin.h $(PREFIX)/Samples/Integration/Projects/linux_x86/_build/Profile/HermeneusOne-Regular.ttf.bin.h $(PREFIX)/Samples/Integration/Projects/linux_x86/_build/Profile/Main.o $(BINDIR)/libNoesis.so $(BINDIR)/libNoesisApp.so
	@echo [LD] $(notdir $@)
	$(Q)g++ $(LFLAGS_PROFILE) -m32 -pie -Wl,-rpath,\$$ORIGIN -Wl,-rpath,$(PREFIX)/../../Bin/linux_x86 -L$(PREFIX)/../../Bin/linux_x86  -o $(BINDIR)/Samples.Integration $(PREFIX)/Samples/Integration/Projects/linux_x86/_build/Profile/Main.o -lNoesisApp -lNoesis
	@echo [STRIP] $(notdir $@)
	$(Q)objcopy --only-keep-debug $(BINDIR)/Samples.Integration $(BINDIR)/Samples.Integration.debug 
	$(Q)strip --strip-unneeded $(BINDIR)/Samples.Integration
	$(Q)objcopy --add-gnu-debuglink=$(BINDIR)/Samples.Integration.debug $(BINDIR)/Samples.Integration

PROFILE_CLEAN_SAMPLES_INTEGRATION:
	@rm -f $(PREFIX)/Samples/Integration/Projects/linux_x86/_build/Profile/Main.o $(PREFIX)/Samples/Integration/Projects/linux_x86/_build/Profile/Settings.xaml.bin.h $(PREFIX)/Samples/Integration/Projects/linux_x86/_build/Profile/HermeneusOne-Regular.ttf.bin.h $(PREFIX)/Samples/Integration/Projects/linux_x86/_build/Profile/pch.h.gch
	@rm -f $(PREFIX)/Samples/Integration/Projects/linux_x86/_build/Profile/pch.h.d $(PREFIX)/Samples/Integration/Projects/linux_x86/_build/Profile/Main.d
	@rm -f $(BINDIR)/Samples.Integration
	@rm -f $(BINDIR)/Samples.Integration.debug

PROFILE_COMPILE_TARGETS += $(BINDIR)/Samples.Integration
PROFILE_ADDITIONAL_DEPS += $(PREFIX)/Samples/Integration/Projects/linux_x86/_build/Profile/pch.h.d $(PREFIX)/Samples/Integration/Projects/linux_x86/_build/Profile/Main.d
PROFILE_CLEAN_TARGETS += PROFILE_CLEAN_SAMPLES_INTEGRATION


